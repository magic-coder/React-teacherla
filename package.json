{
  "name": "react-teacherla",
  "version": "1.0.0",
  "description": "记录讲课内容、授课及其课堂情况评价、填写意见与建议;npm填写评价表、分数统计表、相关课程课堂教学质量评价数据查询等。",
  "main": "index.js",
  "scripts": {
    "build:client": "webpack --config build/webpack.config.client.js",
    "build:server": "webpack --config build/webpack.config.server.js",
    "start": "cross-env NODE_ENV=development webpack-dev-server --config build/webpack.config.client.js",
    "clear": "rimraf dist",
    "build": "npm run clear && npm run build:client && npm run build:server"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/TYRMars/React-TLA.git"
  },
  "author": "Jonathan Zhang",
  "license": "MIT",
  "bugs": {
    "url": "https://github.com/TYRMars/React-TLA/issues"
  },
  "homepage": "https://github.com/TYRMars/React-TLA#readme",
  "dependencies": {
    "express": "^4.16.2",
    "react": "^16.2.0",
    "react-dom": "^16.2.0"
  },
  "devDependencies": {
    "babel-core": "^6.26.0",
    "babel-loader": "^7.1.2",
    "babel-preset-es2015": "^6.24.1",
    "babel-preset-es2015-loose": "^8.0.0",
    "babel-preset-react": "^6.24.1",
    "cross-env": "^5.1.3",
    "html-webpack-plugin": "^2.30.1",
    "react-hot-loader": "^4.0.0-beta.15",
    "rimraf": "^2.6.2",
    "webpack": "^3.10.0",
    "webpack-dev-server": "^2.11.0"
  }
}
